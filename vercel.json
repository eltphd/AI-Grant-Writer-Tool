{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "AI_GATEWAY_API_KEY": "@ai_gateway_api_key",
    "REACT_APP_API_URL": "https://ai-grant-writer-tool-production.up.railway.app"
  }
} 